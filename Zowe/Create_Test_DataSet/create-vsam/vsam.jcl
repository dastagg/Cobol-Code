//Z81187V   JOB 1,NOTIFY=&SYSUID,
// MSGCLASS=H,MSGLEVEL=(1,1),REGION=144M
//* THE FOLLOWING HLQ SYMBOLIC MUST CONTAIN THE HIGH LEVEL
//* QUALIFIER UNDER WHICH THE &PROJECT. DATASETS MAY RESIDE.
//*
//    SET HLQ='Z81187'
//    SET PROJ1='TEST'
//    SET FILE1='CUSTDATA'
//*
//    SET PROJ2='TEST'
//    SET FILE2='VCUST'
//* Reminder: Within the SYSIN def: 
//*           only &SYSUID, 
//*  no other symbolics. Dont use &HLQ
//S1   EXEC PGM=IDCAMS
//SYSPRINT  DD SYSOUT=*
//SYSIN     DD *,SYMBOLS=CNVTSYS
  DELETE &SYSUID..TEST.VCUST
  SET MAXCC=0
  DEFINE CLUSTER ( NAME ( &SYSUID..TEST.VCUST ) -
            VOLUME(VPWRKB) TRACKS(15) RECORDSIZE(157,157) -
            INDEXED KEYS(4 0) -
            REUSE SHAREOPTIONS(2) SPANNED SPEED -
            CONTROLINTERVALSIZE(4096) )
//S2   EXEC PGM=IDCAMS
//SYSPRINT  DD SYSOUT=*
//I1        DD DISP=SHR,DSN=&SYSUID..&PROJ1..&FILE1.
//O1        DD DISP=SHR,DSN=&SYSUID..&PROJ2..&FILE2.
//SYSIN     DD *
  REPRO INFILE(I1) OUTFILE(O1)
//*
//* End of job
